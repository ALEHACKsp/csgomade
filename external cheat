						GUICtrlSetImage($Allbon, @TempDir & "\Active.bmp")
					Else
						GUICtrlSetImage($Allbon, @TempDir & "\Disable.bmp")
					EndIf

					If $treadenablevalue = 1 Then
						GUICtrlSetImage($Trigon, @TempDir & "\Active.bmp")
					Else
						GUICtrlSetImage($Trigon, @TempDir & "\Disable.bmp")
					EndIf


